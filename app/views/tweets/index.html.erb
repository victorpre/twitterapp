<div class="container">
  <%@tweets.each do |tweet|%>
    <div class="row">
      <div class="card">
        <div class="card-content">
          <div class="card-header-blue">
               <h1 class="card-heading"><%= "#{tweet["name"]}"%></h1>
               <i><span class="username help-block"><%="@#{tweet["screen_name"]}"%></span></i>
            </div>
            <div class="card-body">
            <p> <%= tweet["text"]%></p>
          </div>
        </div>
      </div>
    </div>
  <%end%>
</div>
